<h2>プロフィール編集</h2>
<%= form_with model:@user, local:true do |f| %>
  <h3><label for="inputName">名前</label><%= f.text_field :name, autofocus: true, id:"inputName", placeholder:"名前"%></h3>
  <h3><label for="inputImage">プロフィール画像</label><%= f.attachment_field :profile_image, placeholder: "プロフィール画像" %></h3>
  <h3><label for="inputEmail">メールアドレス</label><%= f.email_field :email, placeholder: "メールアドレス", autofocus: true, autocomplete: "email" %></h3>
  <%= f.submit "変更を保存" %>
<% end %>